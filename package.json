  "scripts": {
    "clean": "rm -rf build",
    "build:static": "copyfiles -u 1 src/index.html build",
    "build:app": "rollup -c",
    "build:vendor": "rollup -c rollup.vendor.config.js",
    "watch:app": "npm run build:app -- -w",
    "watch:vendor": "npm run build:vendor -- -w",
    "watch": "parallelshell 'npm run watch:app' 'npm run watch:vendor' 'gulp proxy'",
    "start": "npm run clean -s && npm run build:static -s && npm run watch -s"
  },
